{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": 0,
    "quotes": [1, "single"],
    "import/prefer-default-export": "off",
    "no-trailing-spaces": "off",
    "indent": "off",
    "no-undef": "off",
    "import/extensions": "off",
    "object-curly-newline": "off"
  }
}
